        ${2}
      }

      export default ${1:Class};
    """

  'React Pure Function Component':
    'prefix': 'rfcomponent'
    'description': 'React Pure Function Component'
    'body': """
      import React, { PropTypes } from 'react';

      const ${1:Class} = (${2:props}) => {
        return (
          <div>
            ${4}
          </div>
        );
      };

      ${1:Class}.propsTypes = {
        ${3}
      }

      export default ${1:Class};
    """


'.source.js':
  'React Redux ES6 Container':
    'prefix': 'rcontainer'
    'description': 'React Redux Container'
    'body': """
      import React, { Component, PropTypes } from 'react';
      import { connect } from 'react-redux';

      class ${1:Class} extends Component {
        render() {
          return (
            <div>
              ${5}
            </div>
          );
        }
      }

      ${1:Class}.propsTypes = {
        ${4}
      }

      export default connect(${2}, ${3})(${1:Class});
    """

  'React ES6 Component':
